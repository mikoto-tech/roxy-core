{
    "modelName": "Artwork",

    "resourcesAlgorithmName": "RoxySmoothWeightedObjectAlgorithm",
    "resources": [
        {
            "weight": 1,
            "resourceName": "PixivOriginalResource",
            "resourceAlgorithm": {
                "algorithmName": "RoxyStaticObjectAlgorithm",
                "objectAlgorithmValues": [
                    {
                        "type": "net.mikoto.roxy.core.model.network.resource.HttpTarget",
                        "paramTypes": ["java.lang.String", "java.lang.String"],
                        "values": ["https://www.pixiv.net", "/ajax/illust/${id}"]
                    }
                ]
            }
        },
        {
            "weight": 3,
            "resourceName": "PixivForwardResource",
            "resourceAlgorithm": {
                "algorithmName": "RoxySmoothWeightedObjectAlgorithm",
                "objectAlgorithmValues": [
                    {
                        "type": "net.mikoto.roxy.core.model.network.resource.CurrentWeightedHttpTarget",
                        "paramTypes": ["java.lang.String", "java.lang.String", "java.lang.Integer"],
                        "values": ["https://pixiv-forward-test-1.com", "/artwork/getInformation?artworkId=${id}", 5]
                    },
                    {
                        "type": "net.mikoto.roxy.core.model.network.resource.CurrentWeightedHttpTarget",
                        "paramTypes": ["java.lang.String", "java.lang.String", "java.lang.Integer"],
                        "values": ["https://pixiv-forward-test-2.com", "/artwork/getInformation?artworkId=${id}", 10]
                    },{
                        "type": "net.mikoto.roxy.core.model.network.resource.CurrentWeightedHttpTarget",
                        "paramTypes": ["java.lang.String", "java.lang.String", "java.lang.Integer"],
                        "values": ["https://pixiv-forward-test-3.com", "/artwork/getInformation?artworkId=${id}", 15]
                    }
                ]
            }
        }
    ],

    "task": {
        "threadPoolConfig": {
            "usingDefaultConfig": true
        },
        "taskAlgorithm": {
            "algorithmName": "RoxyIntegerIncrementAlgorithm",
            "algorithmValues": [1, 100]
        },
        "taskCount": 20
    }
}